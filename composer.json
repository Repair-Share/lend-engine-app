{
    "name": "lend-engine/lend-engine-app",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": { "": "src/" },
        "files": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": "~7.4.0",
        "symfony/symfony": "3.4.*",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "sensio/framework-extra-bundle": "^5.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "friendsofsymfony/user-bundle": "~2.0",
        "knplabs/knp-menu-bundle": "~2",
        "twbs/bootstrap": "^3.3",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "wildbit/postmark-php": "^2.6",
        "league/flysystem-aws-s3-v3": "dev-master",
        "oneup/flysystem-bundle": "@stable",
        "oneup/uploader-bundle": "1.9.3",
        "stripe/stripe-php": "6.*",
        "sensio/distribution-bundle": "^5.0",
        "staffim/rollbar-bundle": "^1.3",
        "ahmedsamy/hype-mailchimp-bundle": "~1.3",
        "php-amqplib/rabbitmq-bundle": "^1.12",
        "symfony/serializer": "^3.2",
        "symfony/property-info": "^3.2",
        "guzzlehttp/guzzle": "^6.3",
        "php-heroku-client/php-heroku-client": "^1.3",
        "laminas/laminas-code": "^3.4",
        "kriswallsmith/buzz": "~0.16.1",
        "drewm/mailchimp-api": "^2.5",
        "twilio/sdk": "^6.7",
        "giggsey/libphonenumber-for-php": "^8.12",
        "swiftmailer/swiftmailer": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "symfony/maker-bundle": "^1.14",
        "phpmailer/phpmailer": "^6.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "chown -R herokuishuser:herokuishuser /app/var/cache/prod"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "chown -R herokuishuser:herokuishuser /app/var/cache/prod"
        ],
        "warmup": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
