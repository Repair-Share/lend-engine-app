<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>{% block title %}Lend Engine{% endblock %}</title>

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    {% stylesheets
        '@AppBundle/Resources/css/bootstrap.css'
        '@AppBundle/Resources/admin/css/plugins/select2.css'
        '@AppBundle/Resources/admin/css/plugins/jquery.growl.css'
        '@AppBundle/Resources/admin/css/plugins/datatables.css'
        '@AppBundle/Resources/admin/css/plugins/dataTables.fontAwesome.css'
        '@AppBundle/Resources/admin/css/plugins/buttons.dataTables.min.css'
        '@AppBundle/Resources/admin/css/AdminLTE.2.4.15.css'
        '@AppBundle/Resources/admin/css/skin-black-light.min.css'
        '@AppBundle/Resources/admin/css/main.css'
        '@AppBundle/Resources/admin/css/plugins/daterange.css'
        '@AppBundle/Resources/admin/css/plugins/dropzone.css'
        '@AppBundle/Resources/admin/css/plugins/pace.css'
        '@AppBundle/Resources/css/shared.css'
        filter='cssrewrite'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <style>
        .header-bar {
            padding: 10px 20px;
            background-color: #d2f3ff;
            color: #294353;
        }
        .dt-buttons {
            padding: 5px 15px;
        }
        @media print {
            a[href]:after {
                content: none !important;
            }
        }
    </style>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

    {% block pageCss %}{% endblock %}

