    {% endif %}

    <script>
        var pageLoadComplete = false; // prevent modals from opening until page has loaded. Unset by Pace.js
        var selectContactPath = "{{ path('select_contact') }}";
        var noteDeletePath = "{{ path('note_delete') }}";
        var ajaxDeletePath = "{{ path('ajax_delete') }}";
        var stripePublicApiKey = "{{ tenantInformation.stripePublicApiKey }}";
        var stripePaymentMethodId = "{{ tenantInformation.stripePaymentMethodId }}";
        var stripePaymentFee = "{{ tenantInformation.stripeFee|number_format(2) }}";
        var currencyIsoCode = "{{ tenantInformation.currency }}";
        var currencySymbol = "{{ tenantInformation.currencySymbol }}";
        var minimumPaymentAmount = {{ tenantInformation.minimumPaymentAmount|number_format(2, '.', '')  }};
        var orgName = "{{ tenantInformation.companyName }}";
        var pendingPaymentType = "{{ app.session.get('pendingPaymentType') }}";
    </script>

</head>

<body class="hold-transition skin-black-light sidebar-mini fixed">

<div class="wrapper">

    <header class="main-header">

        <a href="{{ path('homepage') }}" class="logo hidden-xs">
            <span class="logo-mini"> </span>
            <span class="logo-lg">
                <strong style="color:#3f3b55">Lend</strong> Engine
                <span style="font-size: 11px;">Admin</span>
            </span>
        </a>

        <nav class="navbar navbar-static-top" role="navigation">
            <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>

            <div style="display: inline; font-size: 22px; line-height: 50px; padding-left:20px; ">{% block pageTitle %}Lend Engine{% endblock %}</div>
            <div class="pull-right" style="display: inline; padding:10px; ">
                {% block primaryButton %}{% endblock %}
            </div>
        </nav>

    </header>

    <aside class="main-sidebar">
        <section class="sidebar">
            {{ include('partials/admin_sidebar.html.twig') }}
            {% block left %}
                {{ knp_menu_render('adminMenu', {'depth': 2}) }}
            {% endblock %}
        </section>
    </aside>

    <div class="content-wrapper">

        {% if tenantInformation.trialExpiresAt|date("Y-m-d") > date()|date("Y-m-d") and tenantInformation.accountStatus == 'TRIAL' %}
            <div class="trial" style="background-color: #fff7a4; color: #c6c07b; padding: 10px; ">
                Your free trial expires on {{ tenantInformation.trialExpiresAt|date("M jS") }}.
                <a href="{{ path('billing') }}">See plans</a>
            </div>
        {% endif %}

        {% block header_bar %}{% endblock %}

        {% if tenantInformation.trialExpiresAt|date("Y-m-d") < date()|date("Y-m-d") and tenantInformation.accountStatus == 'TRIAL' %}
            <div class="trial" style="background-color: #fff7a4; color: #ff6519; padding: 10px; ">
                <i class="fa fa-exclamation-triangle" style="font-size: 18px; color: #ff6519"></i>
                Your trial expired on {{ tenantInformation.trialExpiresAt|date("M jS") }},
                so your account is in read-only mode.

                {% if app.request.attributes.get('_route') != 'billing' %}
                <div style="padding: 10px 20px">
                    <a href="{{ path('billing') }}" class="btn btn-xs btn-success">Sign up now</a> (there's a free plan)
                </div>
                {% endif %}
            </div>
        {% endif %}

        <section class="content">

            {% for flash_message in app.session.flashbag.get('info') %}
                <div class="alert alert-info disabled">{{ flash_message }}</div>
            {% endfor %}

            {% set errors = app.session.flashbag.get('error') %}

            {% if errors|length > 0 %}
                <div class="alert alert-danger">
                {% for flash_message in errors %}
                    <p>{{ flash_message }}</p>
                {% endfor %}
                </div>
            {% endif %}

            {% block body %}{% endblock %}
            {% block pageHelp %}{% endblock %}
            {% block modal %}{% endblock %}
        </section>

        <div class="modal fade demo-modal" id="modal-wrapper" role="dialog" aria-labelledby="mySmallModalLabel">
            <div class="modal-dialog">
                <div class="modal-content"></div>
            </div>
        </div>
    </div>

    <footer class="main-footer no-print">

        {% set debugs = app.session.flashbag.get('debug') %}
        {% if debugs|length > 0 %}
            <div class="alert alert-info">
                {% for flash_message in debugs %}
                    <p>{{ flash_message }}</p>
                {% endfor %}
            </div>
        {% endif %}

        <div class="pull-right hidden-xs">
            <span style="padding-right: 15px;">
                {% if tenantInformation.plan %}
                    Plan: <a href="{{ path('billing') }}">{{ tenantInformation.plan|capitalize }}</a>
                {% else %}
                    No pay plan yet
                {% endif %}
            </span>
            <strong>{{ tenantInformation.companyName }}</strong>&nbsp;&nbsp;
            ({{ tenantInformation.serverName }})&nbsp;&nbsp;
        </div>
        Logged in as <strong>{{ app.user.firstName }} {{ app.user.lastName }}</strong>
        <a href="{{ path('clear_site') }}">Sign out</a> |
        <a href="{{ path('user', {'id': app.user.id}) }}" class="modal-link">My profile</a> |
        <a href="{{ path("change_password", {'id': app.user.id}) }}" class="modal-link">Change my password</a>
    </footer>

    <aside class="control-sidebar control-sidebar-light">
        sidebar
    </aside>
    <div class="control-sidebar-bg"></div>

</div>

{% javascripts
'@AppBundle/Resources/js/jQuery-3.6.0.min.js'
'@AppBundle/Resources/js/bootstrap.js'
'@AppBundle/Resources/js/jquery.slimscroll.js'
'@AppBundle/Resources/admin/js/plugins/adminlte.2.4.15.js'
'@AppBundle/Resources/js/moment.js'
'@AppBundle/Resources/admin/js/plugins/datatables.js'
'@AppBundle/Resources/admin/js/plugins/datatables.buttons.js'
'@AppBundle/Resources/admin/js/plugins/datatables.buttons.html5.js'
'@AppBundle/Resources/admin/js/plugins/select2.js'
'@AppBundle/Resources/admin/js/plugins/jquery.growl.js'
'@AppBundle/Resources/admin/js/plugins/daterangepicker.min.js'
'@AppBundle/Resources/admin/js/plugins/dropzone.js'
'@AppBundle/Resources/admin/js/plugins/zoom.js'
'@AppBundle/Resources/admin/js/plugins/handlebars.js'
'@AppBundle/Resources/admin/js/plugins/pace.min.js'
filter='uglifyjs2'
%}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

{% if server_name != 'lend-engine-eu' and tenantInformation.stripeAccessToken|length > 0 %}
<script src="https://js.stripe.com/v3/"></script>
{% endif %}

{% javascripts
'@AppBundle/Resources/js/admin.js'
filter='uglifyjs2'
%}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

<script src="/js/payment.js?4bd4a281f4024271ce3d6e11ae20b0e9"></script>

{% block pagejs %}{% endblock %}

{% block javascripts %}{% endblock %}

<script>
    Pace.on("done", function(){
        pageLoadComplete = true;
        // Dashboard choose site link
        if ($("#choose-site-link").html()) {
            $("#choose-site-link").click();
        }
    });
    {% for flash_message in app.session.flashbag.get('success') %}
    $.growl.notice({title: "", message: "{{ flash_message }}"});
    {% endfor %}
</script>

{% if app.environment == 'prod' %}
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-71257503-2', 'auto', {'allowLinker': true});
    ga('require', 'linker');
    ga('linker:autoLink', ['{{ tenantInformation.accountDomain }}'] );
    ga('send', 'pageview', {'dimension1': 'admin', 'dimension2': '{{ tenantInformation.accountCode }}'});
</script>
{% endif %}

</body>
</html>
